* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

a {
    text-decoration: none;
    color: #666;
}

input {
    border: 0 none;
    outline: 0 none;
}

img {
    border: 0 none;
    vertical-align: middle;
}
body{
    background-color: #f2f2f2;
}
/* 头部区域 */
#header {
    width: 100%;
    height: 51px;
    background-color: #ff841d;
}

#header .logo {
    float: left;
}

#header .logo a {
    display: block;
    width: 185px;
    height: 25px;
    margin-top: 13px;
    margin-left: 20px;
}

#header .logo a>img {
    width: 100%;
    height: 100%;
}

#header .app {
    float: right;
    padding-top: 9px;
    margin-right: 20px;
}

#header .app img {
    width: 34px;
    height: 34px;
}

/* 搜索区域 */

#search {
    width: 100%;
    height: 51px;
    background-color: #fff;
    border-bottom: 1px solid #e6e6e6;
}

#search .search {
    width: 100%;
    width: 100%;
}

#search .searchBar {
    float: left;
    width: 89%;
    height: 31px;
}

#search .searchBar input {
    width: 100%;
    height: 100%;
    margin-top: 10px;
    margin-left: 11px;
    background-color: #f1f1f1;
    border-radius: 15px;
    padding-left: 30px;
    font-size: 16px;
}

#search .searchSub {
    float: right;
    margin-top: 11px;
    position: absolute;
    right: 11px;
}

#search .searchSub input {

    width: 63px;
    height: 30px;
    background-color: #ff841d;
    border-radius: 15px;
    font-size: 16px;
    line-height: 30px;
    text-align: center;
    color: #fff;
}

/* 登录区域 */
#main{
    height: 100%;
    width: 100%;
    background-color: #f2f2f2;
    padding: 3px 13px;
}

#main input{
    margin-top: 10px;
    width: 100%;
    height: 38px;
    display: block;
    font-size: 14px;
    border: solid 1px #ccc;
    border-top-color: #aaa;
    padding-left: 5px;
    color: #666;
    background: -webkit-gradient(linear,0 0,0 100%,from(#f5f5f5),to(#fdfdfd));
}

#main img{
    width: 66px;
    height: 38px;
    margin-top: -2px;
}
#main input:nth-child(5){
    display: inline-block;
    width: 100px;
}
#main button{
    width: 40%;
    height: 38px;
    background-color: #d4d4d4;
    background: -webkit-gradient(linear,0 0,0 100%,from(#ffffff),to(#d4d4d4));
    background: -moz-linear-gradient(top, #ffffff, #d4d4d4);
    background: -o-linear-gradient(top,#ffffff, #d4d4d4);
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    -o-border-radius: 5px;
    border-radius: 5px;
    text-align: center;
    font-size: 14px;
    line-height: 32px;
    overflow: hidden;
    border: 1px solid #bababa;
}

#main input:last-child{
    margin-top: 10px;
    background-color: #ff9000;
    background: -webkit-gradient(linear,0 0,0 100%,from(#ffaf47),to(#ff9000));
    border-radius: 5px;
    text-align: center;
    font-size: 20px;
    line-height: 40px;
    overflow: hidden;
    border: none;
    border-bottom: 1px solid #bababa;
    width: 100%;
    color: #fff;
    box-shadow: 0 0 1px #666;
}



/* 底部区域 */
#footer{
    width: 100%;
    background-color: #f2f2f2;
    padding: 40px 0px;
}
#footer .footnav{
    width: 100%;
    height: 34px;
    border-top: 1px solid #c4c4c4;
    border-bottom: 1px solid #c4c4c4;
    margin-top: 10px;
}
#footer .footnav .login{
    float: left;
    width: 30%;
    height: 100%;
    text-align: center;
    font-size: 14px;
    line-height: 34px;   
}
#footer .footnav .login a{
    display: block;
    width: 100%;
    height: 100%;
}
#footer .footnav .reg{
    float: left;
    width: 30%;
    height: 100%;
    text-align: center;
    font-size: 14px;
    line-height: 34px;  
    border-left: 1px solid #c4c4c4;
    border-right: 1px solid #c4c4c4; 
}
#footer .footnav .reg a{
    display: block;
    width: 100%;
    height: 100%;
}
#footer .footnav .toIndex{
    float: left;
    width: 40%;
    height: 100%;
    text-align: center;
    font-size: 14px;
    line-height: 34px;  
}
#footer .footnav .toIndex a{
    display: block;
    width: 100%;
    height: 100%;
}
#footer .footnav .toIndex img{
    margin-top: -3px;
}
#footer .footer{
    width: 100%;
    height: 60px;
    background-color: #f2f2f2;
}
#footer .footer .text{
    width: 100%;
    text-align: center;
    margin-top: 10px;
}
#footer .footer a,span{
    font-size: 12px;
    color: #5a5a5a;
}
#footer .footer a:first-child{
    color: #ff0000;
}
#footer .footer .text p{
    font-size: 13px;
    color: #5a5a5a;
}